	pages = {198--213},
	file = {Elbers et al. - 2007 - Poverty alleviation through geographic targeting .pdf:/Users/j_yamasaki/Zotero/storage/PZDLP73E/Elbers et al. - 2007 - Poverty alleviation through geographic targeting .pdf:application/pdf},
}

