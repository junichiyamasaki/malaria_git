@article{sylla_mosquito_2000,
	title = {Mosquito distribution and entomological inoculation rates in three malaria-endemic areas in {Gabon}},
	volume = {94},
	issn = {0035-9203},
	url = {http://www.sciencedirect.com/science/article/pii/S0035920300902190},
	doi = {10.1016/S0035-9203(00)90219-0},
	abstract = {Mosquitoes were collected during 3 separate periods in 3 areas of different malaria transmission rates in the province of Moyen Ogooué, Gabon, within 1 year (July 1996–May 1997). The campus of the Albert Schweitzer Hospital (HAS) and 2 villages, Bellevue and Tchad, were investigated. A total of 19 836 specimens were collected: 13 122 Mansonia, 3944 Anopheles, 2755 Culex and 15 Aedes were captured. The number of mosquitoes was 7896 and 7995 in July to August and from April to May respectively, and dropped to approximately half in November to December. The individual species showed a different distribution pattern in the 3 study areas. In Tchad we found the lowest number of mosquitoes and also the fewest Anopheles, but when we investigated the number of Plasmodium falciparum-inftcted Anopheles sp. we observed the highest entomological inoculation rate (EIR) there. The EIRs were 23 in HAS, 53 in Bellevue and 61 in Tchad. The method used to determine the number of infected mosquitoes was an enzyme-linked immunosorbent assay (ELISA), confirmed by a polymerase chain reaction-based approach. The ELISA alone revealed too many false-positive mosquitoes.},
	number = {6},
	journal = {Transactions of the Royal Society of Tropical Medicine and Hygiene},
	author = {Sylla, El Hadj Kaba and Kun, Jürgen F.J. and Kremsner, Peter G.},
	year = {2000},
	keywords = {Plasmodium falciparum, Anopheles, Aedes, Culex, ELISA, entomological inoculation rate, Gabon, Mansonia, mosquitoes, polymerase chain reaction},
	pages = {652--656},
	file = {Sylla et al. - 2000 - Mosquito distribution and entomological inoculatio.pdf:/Users/j_yamasaki/Zotero/storage/NS5ZZNP3/Sylla et al. - 2000 - Mosquito distribution and entomological inoculatio.pdf:application/pdf;ScienceDirect Snapshot:/Users/j_yamasaki/Zotero/storage/FQJF3P3E/S0035920300902190.html:text/html},
