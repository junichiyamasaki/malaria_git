
@article{elbers_poverty_2007,
	title = {Poverty alleviation through geographic targeting: {How} much does disaggregation help?},
	volume = {83},
	issn = {03043878},
	shorttitle = {Poverty alleviation through geographic targeting},
	url = {http://linkinghub.elsevier.com/retrieve/pii/S0304387806000150},
	doi = {10.1016/j.jdeveco.2006.02.001},
	abstract = {In this paper, we employ recently completed “poverty maps” for three countries as tools for an ex ante evaluation of the distributional incidence of geographic targeting of public resources. We simulate the impact on poverty of transferring an exogenously given budget to geographically defined sub-groups of the population according to their relative poverty status. We find large gains from targeting smaller administrative units, such as districts or villages. However, these gains are still far from the poverty reduction that would be possible had the planners had access to information on household level income or consumption. Our results indicate that a useful way forward might be to combine fine geographic targeting using a poverty map with within-community targeting mechanisms.},
	language = {en},
	number = {1},
	urldate = {2018-04-04},
	journal = {Journal of Development Economics},
