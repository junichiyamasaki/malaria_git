	file = {malariajournal PDF:/Users/j_yamasaki/Zotero/storage/3QEKXCRG/Ashley et al. - 2009 - Evaluation of three parasite lactate dehydrogenase.pdf:application/pdf;malariajournal Snapshot:/Users/j_yamasaki/Zotero/storage/KFNIGR5H/241.html:text/html},
}

