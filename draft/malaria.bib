@article{cameron_bootstrap-based_2008,
	title = {Bootstrap-{Based} {Improvements} for {Inference} with {Clustered} {Errors}},
	volume = {90},
	issn = {0034-6535},
	url = {http://dx.doi.org/10.1162/rest.90.3.414},
